https://leetcode.com/problems/valid-parentheses/submissions/1808757728/

class Solution {
public:
    bool isValid(string s) {
        if(s.size()%2)return false;
        stack<char> st;

        for(int i = 0; i < s.length();i++){
            char c = s[i];
        

        if(c == '(' || c == '{' || c == '[' ){
            st.push(c);
        }else if(c == ')' || c == '}' || c == ']'){
            if(st.empty()){
                return false;
            }

            char top = st.top();
            st.pop();

            if((c == ')' && top != '(') || (c == '}' && top != '{') || (c == ']' && top != '[')){return false;}
        }
    }

    return st.empty();
}

};
